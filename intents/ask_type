slots:
    room:
        type: PlaceType
        source: $Type
    day:
        type: YANDEX.NUMBER
        source: $DayNumber
    month:
        type: BookTimeMonth
        source: $Month
    time:
        source: $Time
    place:
        type: ItmoPlace
        source: $Where


root:
    %lemma
    $Type [((в | на)? [$DayNumber $Month])? ((в | на)? $Time)? ((в | на)? $Where)?]

$Type:
    $PlaceType
$DayNumber:
    %lemma
    $YANDEX.NUMBER
$Month:
    $BookTimeMonth
$Time:
    %lemma
    $YANDEX.NUMBER $YANDEX.NUMBER
$Where:
    $ItmoPlace
