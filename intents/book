slots:
    day:
        type: YANDEX.NUMBER
        source: $DayNumber
    month:
        type: BookTimeMonth
        source: $Month
    time:
        source: $Time
    room:
        type: PlaceType
        source: $Type
    place:
        type: ItmoPlace
        source: $Where
    today:
        type: TodayAndTomorrow
        source: $TodayAndTomorrow

root:
    %lemma
    $BookWord [((в | на)? [$DayNumber $Month])? ($Type)? ((в | на)? $Time)? ((в | на)? $Where)? ((на)? $TodayAndTomorrow)?]

$BookWord:
    %lemma
    забронируй | забронь | бронь | подать заявку на бронь | забронируй пожалуйста | забронировать
$DayNumber:
    %lemma
    $YANDEX.NUMBER
$Month:
    $BookTimeMonth
$Time:
    %lemma
    $YANDEX.NUMBER $YANDEX.NUMBER
$Where:
    %lemma
    $ItmoPlace
$Type:
    %lemma
    $PlaceType

$Today:
    %lemma
    $TodayAndTomorrow
