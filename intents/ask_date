slots:
    day:
        type: YANDEX.NUMBER
        source: $DayNumber
    month:
        type: BookTimeMonth
        source: $Month
    time:
        source: $Time
    room:
        type: PlaceType
        source: $Type
    place:
        type: ItmoPlace
        source: $Where
    today:
        type: TodayAndTomorrow
        source: $TodayAndTomorrow


root:
    %lemma
    (((на | в)? [$DayNumber $Month]) | ((на)? $TodayAndTomorrow))  [($Type)? ((в | на)? $Time)? ((в | на)? $Where)?]

$DayNumber:
    %lemma
    $YANDEX.NUMBER

$Month:
    $BookTimeMonth
$Time:
    %lemma
    $YANDEX.NUMBER $YANDEX.NUMBER
$Where:
    $ItmoPlace
$Type:
    $PlaceType
$Today:
    %lemma
    $TodayAndTomorrow
