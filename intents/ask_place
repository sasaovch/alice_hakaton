slots:
    place:
        type: ItmoPlace
        source: $Place
    day:
        type: YANDEX.NUMBER
        source: $DayNumber
    month:
        type: BookTimeMonth
        source: $Month
    time:
        source: $Time
    room:
        type: PlaceType
        source: $Type

root:
    %lemma
    (на | в)? $Place [((в | на)? [$DayNumber $Month])? ($Type)? ((в | на)? $Time)?]

$Place:
    $ItmoPlace
$DayNumber:
    %lemma
    $YANDEX.NUMBER
$Month:
    $BookTimeMonth
$Time:
    %lemma
    $YANDEX.NUMBER $YANDEX.NUMBER
$Type:
    $PlaceType
